CsmMenu
{
	name "camera"

	styleSheet "purplestyle"

	CsmItem
	{
		styleClass "Content"

		CsmImage
		{
			CsmCameraImage
			{
			}
		}
	}
	CsmItem
	{
		styleClass "Header"
		CsmTextBlock
		{
			styleClass "H1"
			text "Camera"
		}
	}
	CsmRow
	{
		styleClass "Footer"
		CsmButton
		{
			text "Back"
			onclick "CsmUtils.CloseMenu()"
		}
		CsmButton
		{
			text "Close"
			onclick "CsmUtils.CloseAllMenus()"
		}
	}
}